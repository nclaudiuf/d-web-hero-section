{
  "name": "web-hero-section",
  "version": "2.0.0",
  "description": "Claudiu Furca Portfolio",
  "author": "Claudiu N. Furca https://claudiu.design",
  "license": "ISC",
  "source": "index.html",
  "scripts": {
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o src/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace src/css/styles.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css src/css/styles.css --content index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output src/css/",
    "css": "npm-run-all css-compile css-prefix",
    "start": "parcel index.html --open",
    "build": "parcel build index.html"
  },
  "dependencies": {
    "@parcel/transformer-image": "2.0.0-rc.0",
    "@popperjs/core": "2.9.3",
    "aos": "2.3.4",
    "bootstrap": "5.1.0",
    "jquery": "3.6.0",
    "lozad": "1.16.0"
  },
  "devDependencies": {
    "parcel": "latest",
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0"
  },
  "keywords": [
    "website",
    "static",
    "html-css-js",
    "vanilla"
  ]
}